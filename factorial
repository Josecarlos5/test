def factorial(n):
    """Calculate the factorial of a non-negative integer."""
    if n < 0:
        raise ValueError("Factorial is undefined for negative numbers")
    elif n == 0:
        return 0
    else:
        result = 1
        for i in range(1, n + 1):
            result *= i
        return result

# Example usage of the factorial function
print (factorial(10))
